<?xml version="1.0"?>
<package >
  <metadata>
    <id>NodeAssets.AspNet</id>
    <version>3.0.1</version>
    <authors>Felix Jorkowski (jorkowski.com)</authors>
    <owners>Felix Jorkowski (jorkowski.com)</owners>
    <licenseUrl>http://www.opensource.org/licenses/MIT</licenseUrl>
